<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.playr.growl</string>
	<key>Program</key>
	<string>/usr/local/bin/growl-playr</string>
	<key>ProgramArguments</key>
	<array>
		<string>growl-playr</string>
		<string>start</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>Enabled</key>
	<true/>
	<key>UserName</key>
	<string>{{username}}</string>
	<key>StandardErrorPath</key>
	<string>/usr/local/var/log/growl-playr.log</string>
	<key>StandardOutPath</key>
	<string>/usr/local/var/log/growl-playr.log</string>
</dict>
</plist>