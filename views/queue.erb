<div class="page-header">
	<h1>Playr Queue</h1>
</div>
<div class="row">
	<% @queue.each do |q| %>
	<div class="span10 offset3 queue">
		<h3><%= q.song.title %> <small>by <%= q.song.artist %></small></h3>
		<div class="row">
			<div class="span4"><img src="<%= album_artwork(q.song.album, q.song.artist) %>" alt="<%= q.song.album %>" /></div>
			<div class="span6">
				
			</div>
		</div>
	</div>
	<% end %>
</div>